{
  "plugins": ["@prettier/plugin-xml", "@trivago/prettier-plugin-sort-imports"],
  "xmlWhitespaceSensitivity": "ignore",
  "importOrder": [
    "^(vite|@vitejs)(/(.*))?$",
    "^(react|react-dom)(/(.*))?$",
    "^next(/(.*))?$",
    "^@next(/(.*))?$",
    "^@szhsin/react-accordion(/(.*))?$",
    "^clsx(/(.*))?$",
    "<THIRD_PARTY_MODULES>",
    "^@/assets",
    "^@/components",
    "^@/context",
    "^@/dto",
    "^@/filters",
    "^@/hooks",
    "^@/icons",
    "^@/lib",
    "^@/logo",
    "^@/models",
    "^@/providers",
    "^@/types",
    "^@/utils",
    "^@/styles",
    "^(\\.|\\.\\.)/(.(?!.(css|scss)))*$",
    "\\.(css|scss)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderGroupNamespaceSpecifiers": true
}
